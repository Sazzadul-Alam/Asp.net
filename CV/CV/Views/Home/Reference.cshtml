
@{
    ViewBag.Title = "Reference";
}
<nav class="navbar navbar-expand-lg bg-body-tertiary">
    <div class="container-fluid">
        <a class="navbar-brand" href="#">CV</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="/Home/Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Education">Education</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Projects">Projects</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Reference">Reference</a>
                </li>
            </ul>


        </div>
    </div>
</nav>
<h2>References</h2>

<ul>
    @foreach (var item in Model)
            {
                <li>
                    <b> @item.Name</b>
                    <ul>
                        <li>
                            @item.Occupation
                        </li>
                        <li>
                            @item.Email
                        </li>
                    </ul>
                </li>
            }
</ul>